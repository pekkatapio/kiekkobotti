e":true,"deviceType":"Motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[3],"name":"Maila","customName":true,"deviceType":"Motor","setting":{"reversed":"false","fwd":"close","rev":"open","gear":"ratio18_1"},"triportSourcePort":22},{"port":[],"name":"Controller1","customName":false,"deviceType":"Controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"},"triportSourcePort":22}],"slot":0,"platform":"V5","sdkVersion":"20191206.14.00.00","appVersion":"1.0.8","fileFormat":"1.0.0","icon":"","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"v5.h\"\n#include \"v5_cpp.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\nmotor LeftMotor = motor(PORT1, ratio18_1, true);\n\nmotor RightMotor = motor(PORT10, ratio18_1, false);\n\nmotor Maila = motor(PORT3, ratio18_1, false);\n\ncontroller Controller1 = controller(primary);\n\n\n\n// Generated code.\n\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\nint Brain_precision = 0;\nint Controller1_precision = 0;float myVariable, oikea, vasen, kerroin;\n\nint whenStarted1() {\n  // moottoritehon skaalauskerroin, 1 on täysi teho\n  kerroin = 1.0;\n  Maila.setStopping(vex::brakeType::hold);\n  LeftMotor.setStopping(vex::brakeType::hold);\n  RightMotor.setStopping(vex::brakeType::hold);\n  Maila.setVelocity(100.0, vex::velocityUnits::pct);\n  while (true) {\n    // asetetaan eteen-taakse-tatin arvo vasemman ja oikean moottorin lähtöarvoksi\n    oikea = Controller1.Axis3.position();\n    vasen = Controller1.Axis3.position();\n    // lisätään/vähennetään oikea-vasen-tatin arvo oikean ja vasemman moottorin arvosta\n    // tällä kompensoidaan moottoreiden pyöriminen vastaamaan kääntymistä\n    oikea = oikea + Controller1.Axis1.position();\n    vasen = vasen - Controller1.Axis1.position();\n    // skaalataan arvo kertoimella\n    vasen = round(vasen * kerroin);\n    oikea = round(oikea * kerroin);\n    // asetetaan moottorille nopeus perustuen aikaisempiin laskentoihin\n    LeftMotor.setVelocity(fabs(static_cast<float>(vasen)), vex::velocityUnits::pct);\n    RightMotor.setVelocity(fabs(static_cast<float>(oikea)), vex::velocityUnits::pct);\n    // määritetään moottorien pyörimissuunnat\n    if (vasen > 0.0) {\n      LeftMotor.spin(vex::directionType::fwd);\n    }\n    else {\n      LeftMotor.spin(vex::directionType::rev);\n    }\n    if (oikea > 0.0) {\n      RightMotor.spin(vex::directionType::fwd);\n    }\n    else {\n      RightMotor.spin(vex::directionType::rev);\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\nvoid onevent_Controller1ButtonR1_pressed_0() {\n  Maila.setVelocity(100.0, vex::velocityUnits::pct);\n  Maila.spin(vex::directionType::rev);\n  wait(0.2, seconds);\n  waitUntil((Maila.torque(vex::torqueUnits::InLb) > 5.0));\n  Maila.setVelocity(40.0, vex::velocityUnits::pct);\n  Maila.spin(vex::directionType::fwd);\n  wait(0.2, seconds);\n  waitUntil((Maila.torque(vex::torqueUnits::InLb) > 5.0));\n  Maila.stop();\n}\n\nvoid onevent_Controller1ButtonL1_pressed_0() {\n  if (kerroin < 1.0) {\n    kerroin = kerroin + 0.1;\n  }\n}\n\nvoid onevent_Controller1ButtonL2_pressed_0() {\n  if (kerroin > 0.1) {\n    kerroin = kerroin - 0.1;\n  }\n}\n\nconst char* printToController1_numberFormat() {\n  switch(Controller1_precision){\n  case 0:  return \"%.0f\";\n  case 1:  return \"%.1f\";\n  case 2:  return \"%.2f\";\n  case 3:  return \"%.3f\";\n  case -1: return \"%f\";\n  default: return \"%f\";\n  }\n}\n\nint whenStarted2() {\n  Controller1.Screen.clearScreen();\n  while (true) {\n    wait(0.2, seconds);\n    Controller1.Screen.setCursor(1, 1);\n    Controller1.Screen.print(\"Nopeus: \");\n    Controller1.Screen.print(printToController1_numberFormat(), static_cast<float>(kerroin * 100.0));\n    Controller1.Screen.print(\"  \");\n    Controller1.Screen.setCursor(2, 1);\n    Controller1.Screen.print(\"Akku: \");\n    Controller1.Screen.print(printToController1_numberFormat(), static_cast<float>(Brain.Battery.capacity()));\n    Brain.Screen.print(\"  \");\n  wait(5, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  // register event handlers\n  Controller1.ButtonR1.pressed(onevent_Controller1ButtonR1_pressed_0);\n  Controller1.ButtonL1.pressed(onevent_Controller1ButtonL1_pressed_0);\n  Controller1.ButtonL2.pressed(onevent_Controller1ButtonL2_pressed_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  vex::task ws1(whenStarted2);\n  whenStarted1();\n}"}